{
  "name": "desafio_fullstack",
  "version": "1.0.0",
  "description": "Nuestro proyecto fullstack para el desafio",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run dev --prefix client",
